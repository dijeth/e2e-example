{
  "name": "e2e-example",
  "version": "1.0.0",
  "description": "Example for stack: cucumber, playwright, allure-report, github actions, github pages",
  "scripts": {
    "e2e": "cucumber-js",
    "e2e:report": "cucumber-js --format allure-cucumberjs/reporter --format-options '{ \"resultsDir\": \"allure-results\" }'",
    "e2e:report:serve": "allure serve allure-results"
  },
  "author": "dijeth",
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "^11.2.0",
    "@cucumber/messages": "^27.2.0",
    "@types/node": "^22.13.5",
    "allure-commandline": "^2.32.2",
    "allure-cucumberjs": "^3.2.0",
    "playwright": "^1.50.1",
    "prettier": "^3.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
